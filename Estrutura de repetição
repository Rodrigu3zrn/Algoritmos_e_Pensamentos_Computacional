#include <stdio.h>

int main() {
  int i; 
  for (i = 1; i <= 5; i++) {
    printf("contagem de 1 a 5: %d\n", i);
  }

  printf("fim");
  return 0;
}
#include <stdio.h>

int main() {
  int contador = 0;

printf("digite um numero, maior que 3 = encerra programa: ");
  scanf("%d", &numero);

  while (contador < 3) {
    printf("contagem %d\n", contador + 1);
    contador++;
  }

  printf("fim");
  return 0;
}
#include <stdio.h>

int main() {
  int numero;

  do {
    printf("Digite um numero de 1 a 10: ");
    scanf("%d", &numero);
     
    if (numero < 1 || numero > 10) {
      printf("numero invalido.");
    }
     
  } while (numero < 1 || numero > 10);

  printf("cadastro finalizado", numero);
  return 0;
}
